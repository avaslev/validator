{
    "name": "avaslev/validator",
    "type": "library",
    "description": "A extendable php validator",
    "keywords": ["php", "validator", "valid"],
    "homepage": "https://github.com/avaslev/validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Vasilyev",
            "email": "alex@onlam.ru",
            "homepage": "http://onlamp.ru"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Avaslev\\Validator\\": "src"
        }
    },
    "autoload-test": {
        "psr-4": {
            "Avaslev\\Validator\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
